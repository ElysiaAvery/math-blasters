<a-scene canvas="height: 50; width: 50">
  <a-assets>
    <img id="number1-sprite" crossorigin="" src="/assets/images/number1.gif">
    <img id="number2-sprite" crossorigin="" src="/assets/images/number2.gif">
    <img id="number3-sprite" crossorigin="" src="/assets/images/number3.gif">
    <img id="number4-sprite" crossorigin="" src="/assets/images/number4.gif">
    <img id="number5-sprite" crossorigin="" src="/assets/images/number5.gif">
    <img id="number6-sprite" crossorigin="" src="/assets/images/number6.gif">
    <img id="number7-sprite" crossorigin="" src="/assets/images/number7.gif">
    <img id="number8-sprite" crossorigin="" src="/assets/images/number8.gif">
    <img id="number9-sprite" crossorigin="" src="/assets/images/number9.gif">


    <a-mixin id="laser"
             geometry="primitive: cylinder; radius: 0.08; translate: 0 -2 0"
             material="color: green; metalness: 0.2; opacity: 0.4; roughness: 0.3"
             rotation="90 0 0" projectile collider="target: .enemy"></a-mixin>
  </a-assets>

  <a-entity id='entity' layout="type: circle; radius: 5;" position="0 0.5 0">
    <a-animation attribute="rotation" dur="500000" easing="linear" repeat="indefinite" to="0 360 0"></a-animation>
    <a-image id="-9" class="enemy" position="0 -60 0" look-at="#player" crossorigin="" src="#number9-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>
    <a-image id="-7" class="enemy" position="0 -60 0" look-at="#player" crossorigin="" src="#number7-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>
  </a-entity>




  <a-camera id="player" position="0 1.8 0" spawner="mixin: laser; on: click" click-listener><a-cursor><a-circle color="white" radius=".01" opacity=".1"></a-circle></a-cursor></a-camera>

  <a-sphere radius="40"  position="0 50 0" metalness="0" light="type: ambient" roughness="0.5" color="gold">
    <a-animation attribute="rotation" dur="50000" easing="linear" repeat="indefinite" to="0 0 360"></a-animation>

    <a-image id="1" class="1 enemy" position="0 -50 0" look-at="#player" src="#number1-sprite"  transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="1000" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="1000" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="2" class="2 enemy" position="50 0 0" look-at="#player" crossorigin="" src="#number2-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <!-- <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation> -->
    </a-image>

    <a-image id="3" class="3 enemy" position="-50 0 0" look-at="#player" crossorigin="" src="#number3-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="4" class="4 enemy" position="0 50 0" look-at="#player" crossorigin="" src="#number4-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="5" class="5 enemy" position="50 50 0" look-at="#player" crossorigin="" src="#number5-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="6" class="6 enemy" position="50 -50 0" look-at="#player" crossorigin="" src="#number6-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="7" class="7 enemy" position="-50 -50 0" look-at="#player" crossorigin="" src="#number7-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="8" class="8 enemy" position="-50 50 0" look-at="#player" crossorigin="" src="#number8-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-image id="9" class="9 enemy" position="0 -60 0" look-at="#player" crossorigin="" src="#number9-sprite" transparent="true">
      <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
      <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
    </a-image>

    <a-sphere radius=".1" color="blue" position="0 -60 0">
    </a-sphere>

    <a-animation attribute="rotation" dur="100000" easing="linear" repeat="indefinite" to="360 0 0"></a-animation>
    <a-sphere radius="10" color="teal" light="type: directional; color: white" roughness="0.5" metalness="0.5" position="-20 -100 200">
      <a-animation attribute="rotation" dur="1000" easing="linear" repeat="indefinite" to="0 0 360"></a-animation>
      <a-ring src="/assets/images/rings.jpg" radius-inner="17" radius-outer="12"></a-ring>
    </a-sphere>

    <a-sphere src="/assets/images/moon.jpg" color="purple" radius="20" position="-300 -450 20"></a-sphere>
  </a-sphere>

  <!-- /////////////////////////////////////////////////////////////////////////////////////////// -->

  <a-image id="10" class="1 enemy" position="5 1 5" look-at="#player" src="#number1-sprite"  transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="1000" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="1000" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="11" class="2 enemy" position="-5 0 1" look-at="#player" crossorigin="" src="#number2-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <!-- <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation> -->
  </a-image>

  <a-image id="12" class="3 enemy" position="-5 0 -5" look-at="#player" crossorigin="" src="#number3-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="13" class="4 enemy" position="3 -5 -5" look-at="#player" crossorigin="" src="#number4-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="14" class="5 enemy" position="5 2 -3" look-at="#player" crossorigin="" src="#number5-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="15" class="6 enemy" position="5 -5 4" look-at="#player" crossorigin="" src="#number6-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="16" class="7 enemy" position="-5 -5 0" look-at="#player" crossorigin="" src="#number7-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="17" class="8 enemy" position="-5 5 0" look-at="#player" crossorigin="" src="#number8-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>

  <a-image id="18" class="9 enemy" position="6 0 3" look-at="#player" crossorigin="" src="#number9-sprite" transparent="true">
    <a-animation attribute="opacity" begin="collider-hit" dur="400" ease="linear" from="1" to="0"></a-animation>
    <a-animation attribute="scale" begin="collider-hit" dur="400" ease="linear" to="0 0 0"></a-animation>
  </a-image>


  <a-sky src="/assets/images/milkyway.jpg"></a-sky>
</a-scene>

{{input value=comparison id="comparison"}}
